{
  "name": "Developer Agent",
  "version": "1.0.0",
  "description": "Multi-Agent System Developer Agent",
  "type": "agent",
  "capabilities": [
    "frontend_development",
    "react_development",
    "nextjs_development",
    "react_native_development",
    "ui_development",
    "component_development"
  ],
  "configuration": {
    "supported_languages": [
      "typescript",
      "javascript",
      "jsx",
      "tsx"
    ],
    "frameworks": [
      "react",
      "next.js",
      "react-native"
    ],
    "max_file_size": 1000000,
    "timeout": 60000,
    "log_level": "info"
  },
  "workflow": {
    "development": {
      "component_development": {
        "create_components": true,
        "implement_ui": true,
        "add_styling": true
      },
      "feature_development": {
        "implement_features": true,
        "add_functionality": true,
        "integrate_apis": true
      },
      "optimization": {
        "performance_optimization": true,
        "code_optimization": true,
        "accessibility_improvement": true
      }
    }
  },
  "rules": {
    "development": "Modern React/Next.js standartlarına uygun geliştirme yapmalı",
    "performance": "Performans optimizasyonları uygulamalı",
    "accessibility": "Accessibility standartlarına uygun kod yazmalı"
  }
}
